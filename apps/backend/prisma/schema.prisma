generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model SportsArticle {
  id        String   @id @default(uuid())
  title     String
  content   String
  imageUrl  String?
  createdAt DateTime @default(now())
  deletedAt DateTime?

  @@index([createdAt])
  @@index([deletedAt])
}
